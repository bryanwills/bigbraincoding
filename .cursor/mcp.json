{
    "mcpServers": {
      "filesystem": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-filesystem",
          "/home/bryanwi09/",
          "/var/www/bigbraincoding.com/html",
          "/var/www/bryanwills.dev/html"
        ]
      },
      "github": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-github"
        ],
        "env": {
          "GITHUB_PERSONAL_ACCESS_TOKEN": "{GITHUB_PERSONAL_ACCESS_TOKEN}"
        }
      },
      "supabase": {
        "command": "npx",
        "args": [
          "-y",
          "@supabase/mcp-server-supabase@latest",
          "--access-token",
          "{SUPABASE_ACCESS_TOKEN}"
        ]
      },
      "mcp-installer": {
        "command": "npx",
        "args": [
          "@anaisbetts/mcp-installer"
        ]
      },
      "sequential-thinking": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-sequential-thinking"
        ]
      },
      "shadcn-ui": {
      "command": "npx",
      "args": [
        "@jpisnice/shadcn-ui-mcp-server",
        "--github-api-key",
        "{SHADCN_UI_GITHUB_API_KEY}"
    ]
    },
    "@21st-dev/magic": {
      "command": "npx",
      "args": [
        "-y",
        "@21st-dev/magic@latest",
        "API_KEY=\"${MAGIC_API_KEY}\""
      ]
    }
    },
    "globalShortcut": "Alt+`"
  }
